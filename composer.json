{
    "name": "juve534/lgtm-generator",
    "description": "LGTM Generator",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Juve534\\LgtmGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "bref/bref": "^1.2",
        "aws/aws-sdk-php": "^3.178"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "authors": [
        {
            "name": "juve534",
            "email": "maxim_4141.rock@zoho.com"
        }
    ],
    "scripts": {
        "cs-check": [
            "@php ./vendor/bin/php-cs-fixer fix --dry-run --diff --ansi"
        ],
        "cs-fix": [
            "@php ./vendor/bin/php-cs-fixer fix"
        ]
    }
}
